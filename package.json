{
  "name": "react-jest",
  "version": "1.0.0",
  "author": "Prasanna KC Dommalapati",
  "description": "Simple Webpack dev server, running react and tests with jest",
  "license": "MIT",
  "main": "webpack.config.js",
  "scripts": {
    "test": "jest",
    "test:dev": "jest --watch",
    "test:coverage": "jest --coverage",
    "start": "webpack-dev-server",
    "build": "jest && webpack -p"
  },
  "jest": {
    "transform": {
      ".*": "./node_modules/jest-babel-webpack-react"
    },
    "setupFiles": [
      "./src/setupTests.js"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.3.2",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^2.0.1",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "enzyme-to-json": "^3.3.5",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^19.0.2",
    "jest-babel-webpack-react": "^0.1.4",
    "jest-css-modules": "^1.1.0",
    "node-sass": "^4.11.0",
    "react-test-renderer": "^16.6.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.27.1",
    "webpack-dev-server": "^3.1.10"
  },
  "dependencies": {
    "dotenv": "^4.0.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3"
  }
}
